macro_paste_special: macro_paste_special{
compatible = "zmk,behavior-macro";
label = "macro_paste_special";
#binding-cells = <0>;
bindings
    = <&macro_press &kp LA(LEFT_COMMAND) &kp LEFT_SHIFT>
    , <&macro_tap &kp V>
    , <&macro_release &kp LA(LEFT_COMMAND) &kp LEFT_SHIFT>
    ;
};

macro_go_right_tab: macro_go_right_tab{
compatible = "zmk,behavior-macro";
label = "macro_go_right_tab";
#binding-cells = <0>;
bindings
    = <&macro_press &kp LG(LEFT_SHIFT)>
    , <&macro_tap &kp RIGHT_BRACKET>
    , <&macro_release &kp LG(LEFT_SHIFT)>
    ;
};

macro_go_left_tab: macro_go_left_tab{
compatible = "zmk,behavior-macro";
label = "macro_go_left_tab";
#binding-cells = <0>;
bindings
    = <&macro_press &kp LG(LEFT_SHIFT)>
    , <&macro_tap &kp LEFT_BRACKET>
    , <&macro_release &kp LG(LEFT_SHIFT)>
    ;
};

macro_m1: macro_m1{
compatible = "zmk,behavior-macro";
label = "macro_m1";
#binding-cells = <0>;
bindings = <&kp LS(K)>, <&kp I>, <&kp N>, <&kp E>, <&kp S>, <&kp I>, <&kp S>;
};
