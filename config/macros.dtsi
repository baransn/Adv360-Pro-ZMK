macro_paste_special: macro_paste_special{
compatible = "zmk,behavior-macro";
#binding-cells = <0>;
bindings
    = <&macro_press &kp LEFT_COMMAND &kp LEFT_SHIFT &kp LEFT_ALT>
    , <&macro_tap &kp V>
    , <&macro_release &kp LEFT_COMMAND &kp LEFT_SHIFT &kp LEFT_ALT>
    ;
};
macro_go_right_tab: macro_go_right_tab{
compatible = "zmk,behavior-macro";
label = "macro_go_right_tab";
#binding-cells = <0>;
bindings
    = <&macro_press &kp LEFT_COMMAND &kp LEFT_SHIFT>
    , <&macro_tap &kp RIGHT_BRACKET>
    , <&macro_release &kp LEFT_COMMAND &kp LEFT_SHIFT>
    ;
};
macro_go_left_tab: macro_go_left_tab{
compatible = "zmk,behavior-macro";
label = "macro_go_left_tab";
#binding-cells = <0>;
bindings
    = <&macro_press &kp LEFT_COMMAND &kp LEFT_SHIFT>
    , <&macro_tap &kp LEFT_BRACKET>
    , <&macro_release &kp LEFT_COMMAND &kp LEFT_SHIFT>
    ;
};
macro_m1: macro_m1{
compatible = "zmk,behavior-macro";
label = "macro_m1";
#binding-cells = <0>;
bindings = <&kp LS(K)>, <&kp I>, <&kp N>, <&kp E>, <&kp S>, <&kp I>, <&kp S>;
};
