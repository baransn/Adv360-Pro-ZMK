macro_paste_special: macro_paste_special{
compatible = "zmk,behavior-macro";
label = "macro_paste_special";
#binding-cells = <0>;
bindings = <&kp LEFT_ALT>, <&kp LEFT_COMMAND>, <&kp LEFT_SHIFT>, <&kp V>;
};
macro_go_right_tab: macro_go_right_tab{
compatible = "zmk,behavior-macro";
label = "macro_go_right_tab";
#binding-cells = <0>;
bindings = <&kp LEFT_COMMAND>, <&kp LEFT_SHIFT>, <&kp RIGHT_BRACKET>;
};
macro_go_left_tab: macro_go_left_tab{
compatible = "zmk,behavior-macro";
label = "macro_go_left_tab";
#binding-cells = <0>;
bindings = <&kp LEFT_COMMAND>, <&kp LEFT_SHIFT>, <&kp LEFT_BRACKET>;
};
macro_alt_backslash: macro_alt_backslash{
compatible = "zmk,behavior-macro";
label = "macro_alt_backslash";
#binding-cells = <0>;
bindings = <&kp LEFT_ALT>, <&kp BACKSLASH>;
};
macro_m1: macro_m1{
compatible = "zmk,behavior-macro";
label = "macro_m1";
#binding-cells = <0>;
bindings = <&kp LS(K)>, <&kp I>, <&kp N>, <&kp E>, <&kp S>, <&kp I>, <&kp S>;
};
